{
  "name": "solarhijri-js",
  "version": "1.0.0",
  "description": "Converts Gregorian and SolarHijri calendars to each other",
  "main": "dist/solarHijri-js.common.js",
  "module": "dist/solarHijri-js.esm.js",
  "typings": "types/solarHijri-js.d.ts",
  "directories": {
    "dist": "dist",
    "types": "types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xsoh/solarHijri-js"
  },
  "keywords": [
    "solarHijri",
    "hijri",
    "hijriah",
    "arabic",
    "lunar",
    "Umm al-Qura",
    "shamsi",
    "calendar",
    "date"
  ],
  "scripts": {
    "test": "mocha --reporter spec --ui bdd --colors --check-leaks test.js",
    "compile": "node_modules/.bin/tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run compile && node build"
  },
  "author": "Suhail Alkowaileet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xsoh/solarHijri-js/issues"
  },
  "homepage": "https://github.com/xsoh/solarHijri-js",
  "dependencies": {},
  "devDependencies": {
    "mocha": "^3.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "should": "^10.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.5"
  }
}
